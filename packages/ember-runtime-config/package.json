{
  "name": "ember-runtime-config",
  "version": "1.0.1",
  "description": "Configure an Ember application with server-side environment variables",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexlafroscia/ember-runtime-config.git"
  },
  "license": "MIT",
  "author": "Alex LaFroscia",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "GREETING='Hello, world!' ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "standard-version"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "theredoc": "^1.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.5.0",
    "@glimmer/component": "^1.1.1",
    "@glimmer/tracking": "^1.1.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.1",
    "ember-cli": "~4.3.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^3.3.0",
    "ember-cli-fastboot-testing": "^0.6.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "express": "^4.17.3",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.18.1",
    "qunit-dom": "^2.0.0",
    "rsvp": "^4.8.5",
    "standard-version": "^9.3.2",
    "webpack": "^5.72.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-dotenv"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
