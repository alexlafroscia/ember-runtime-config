{
  "name": "ember-runtime-config",
  "version": "1.0.1",
  "description": "Configure an Ember application with server-side environment variables",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/alexlafroscia/ember-runtime-config/",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexlafroscia/ember-runtime-config.git",
    "directory": "packages/ember-runtime-config"
  },
  "license": "MIT",
  "author": "Alex LaFroscia",
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.js",
    "./blueprints": "./blueprints/*",
    "./fastboot": "./fastboot/index.js",
    "./lib": "./lib/*",
    "./middleware": "./middleware/index.js",
    "./test-support": "./dist/test-support/index.js",
    "./registry": "./types/registry.d.ts",
    "./template-registry": "./types/template-registry.d.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ],
      "registry": [
        "types/registry.d.ts"
      ],
      "template-registry": [
        "types/template-registry.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.js",
    "blueprints",
    "dist",
    "fastboot",
    "lib",
    "middleware",
    "types/registry.d.ts",
    "types/template-registry.d.ts"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepublishOnly": "rollup --config",
    "release": "standard-version",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "theredoc": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-syntax-decorators": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@embroider/addon-dev": "^3.0.0",
    "@glint/core": "^0.9.7",
    "@glint/environment-ember-loose": "^0.9.7",
    "@tsconfig/ember": "^1.1.0",
    "@types/ember": "^4.0.2",
    "@types/ember__application": "^4.0.4",
    "@types/ember__array": "^4.0.3",
    "@types/ember__component": "^4.0.0",
    "@types/ember__controller": "^4.0.4",
    "@types/ember__debug": "^4.0.3",
    "@types/ember__destroyable": "^4.0.1",
    "@types/ember__engine": "^4.0.0",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.5",
    "@types/ember__owner": "^4.0.2",
    "@types/ember__polyfills": "^4.0.1",
    "@types/ember__routing": "^4.0.0",
    "@types/ember__runloop": "^4.0.2",
    "@types/ember__service": "^4.0.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.1",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.8.2",
    "@types/ember__utils": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rollup": "^3.5.1",
    "rollup-plugin-ts": "^3.0.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "@ember/test-helpers": "*"
  },
  "peerDependenciesMeta": {
    "@ember/test-helpers": {
      "optional": true
    }
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "versionCompatibility": {
      "ember": "3.24 || 3.28 || >=4.0"
    },
    "after": [
      "ember-cli-dotenv"
    ],
    "app-js": {
      "./helpers/runtime-config.js": "./dist/_app_/helpers/runtime-config.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
